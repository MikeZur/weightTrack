public class Pers {
	
	static int cal = 0;
	static int sugar = 0;
	static int prot = 0;
	static int carb = 0;
	static int fat = 0;
	static int goal;
	
	public Pers(){
		
	}
	
	public void setGoal(int a){
		goal = a;
	}
	
	public int getCal(){
		return cal;
	}
	
	public int getSugar(){
		return sugar;
	}
	
	public int getProt(){
		return prot;
	}
	
	public int getCarb(){
		return carb;
	}
	
	public int getFat(){
		return fat;
	}
	
	public int getGoal(){
		return goal;
	}
	
	public void addCal(int a){
		cal += a;
	}
	
	public void addSugar(int a){
		sugar += a;
	}
	
	public void addProt(int a){
		prot += a;
	}
	
	public void addCarb(int a){
		carb += a;
	}
	public void addFat(int a){
		fat += a;
	}
}
