public class Pers {
	
	int cal = 0;
	int sugar = 0;
	int prot = 0;
	int carb = 0;
	int fat = 0;
	int goal = 0;
	boolean gender;
	String activityLvl;
	
	public Pers(){
		Goal.go();
		goal = Goal.goal;
	}
	public Pers(String info){
		
	}
	
	public void setGoal(int a){
		goal = a;
	}
	
	public int getCal(){
		return cal;
	}
	
	public int getSugar(){
		return sugar;
	}
	
	public int getProt(){
		return prot;
	}
	
	public int getCarb(){
		return carb;
	}
	
	public int getFat(){
		return fat;
	}
	
	public int getGoal(){
		return goal;
	}
	
	public void addCal(int a){
		cal += a;
	}
	
	public void addSugar(int a){
		sugar += a;
	}
	
	public void addProt(int a){
		prot += a;
	}
	
	public void addCarb(int a){
		carb += a;
	}
	public void addFat(int a){
		fat += a;
	}
	public String toString(){
		String s = (cal +" "+ sugar +" "+ prot +" "+ carb +" "+ fat +" "+ goal +" "+ gender +" "+ activityLvl);
		return s;
	}
}
